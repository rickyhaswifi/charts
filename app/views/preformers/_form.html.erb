<h1>Artist Details</h1>
<h1><%= @preformer.id ? 'Edit Artist' : 'Create Artist'%> </h1>

<%= form_for [@song, @preformer] do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %>
    
    <%= f.label :label %>
    <%= f.text_field :label %>

    <%= f.submit%>
<% end %>

<hr>

<button>
<%= link_to 'Cancel', root_path %>
</button>
