<h1>Add a Chart</h1>
<h1> <%= @chart.id ? 'Edit Chart' : 'Create Chart'%> </h1>

<%= form_for @chart do |f| %>

    <%= f.label :title %>
    <%= f.text_field :title %>
    
    <%= f.label :genre %>
    <%= f.text_field :genre %>
    <%= f.submit%>
<% end%>


<hr>

<button>
<%= link_to 'Cancel', root_path%>
</button>
